
public class TestaWhile {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int contador = 1;
        while(contador <= 10) {
            System.out.println(contador);
            contador = contador + 1;
            
            contador = contador * contador;
            
            System.out.println(contador + "x" + contador);
        }
	}

}
