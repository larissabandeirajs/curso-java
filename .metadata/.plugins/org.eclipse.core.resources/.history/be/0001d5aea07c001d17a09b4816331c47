package bytebank;
public class Conta {
    double saldo;
    int agencia;
    int ContaCorrente;
    String titular;
    
   public void deposita(double valor) {
    	
    	this.saldo = this.saldo + valor;
    }
   
   public void retirada(double valor){
   			
	   	this.saldo = this.saldo - valor;
   		 	
   			
     }
   
}